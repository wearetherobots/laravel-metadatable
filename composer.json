{
    "name": "watr/laravel-metadatable",
    "description": "Laravel package for adding key value metadata relationship.",
    "keywords": [
        "laravel",
        "metadata",
        "eloquent"
    ],
    "support": {
        "issues": "https://github.com/wearetherobots/laravel-metadatable/issues",
        "source": "https://github.com/wearetherobots/laravel-metadatable"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aftab Hussain Miranda",
            "email": "wafto.mx@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/console": "5.6.x",
        "illuminate/contracts": "5.6.x",
        "illuminate/database": "5.6.x",
        "illuminate/support": "5.6.x"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "WATR\\Metadatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WATR\\Metadatable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WATR\\Metadatable\\MetadatableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
